{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "authorStatus": {
      "type": "enumeration",
      "enum": [
        "None",
        "Requested confirmation",
        "Confirmed"
      ],
      "default": "None"
    },
    "company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::aanbieder.aanbieder",
      "mappedBy": "user"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "user"
    },
    "favorites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::aanbieder.aanbieder"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "AANBIEDER",
        "CONSUMENT"
      ]
    }
  }
}
