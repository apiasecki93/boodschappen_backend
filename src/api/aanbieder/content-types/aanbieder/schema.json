{
  "kind": "collectionType",
  "collectionName": "aanbieders",
  "info": {
    "singularName": "aanbieder",
    "pluralName": "aanbieders",
    "displayName": "Aanbieder",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "naam": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "company"
    },
    "adres": {
      "type": "component",
      "repeatable": false,
      "component": "global.address"
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "global.contact"
    },
    "beschrijving": {
      "type": "richtext"
    },
    "beschrijving_en": {
      "type": "richtext"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "company"
    },
    "review_score": {
      "type": "float"
    },
    "aanbod": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::aanbod.aanbod",
      "mappedBy": "aanbieder"
    },
    "openingstijden": {
      "type": "component",
      "repeatable": false,
      "component": "global.openinghours"
    },
    "coordinaten": {
      "type": "component",
      "repeatable": false,
      "component": "global.coordinates"
    }
  }
}
